    "dotenv": "^16.0.3",
    "electron-reload": "^2.0.0-alpha.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "electron": "^21.2.0",
    "electron-reloader": "^1.2.3",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.4.1",
