{
  "name": "khartes",
  "version": "1.0.0",
  "description": "",
  "main": "/electron/electron.ts",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "start": "concurrently \"npx tsc \" \"nodemon -q ./dist/server/index.js\" \"webpack --config ./webpack.production.js\" \"electron ./dist/electron.js\"",
    "start2": "npm run express-dev & npm run build && ENV=development electron ./dist/electron.js",
    "tsc": "tsc -p .",
    "front-dev": "concurrently \"npx tsc \" \"webpack --config ./webpack.config.js\" \"electron ./dist/electron.js\"",
    "start:dev": "npx tsc & webpack-dev-server --open --config webpack.config.js",
    "express-build": "npx tsc",
    "express-start": "node dist/server/index.js",
    "express-dev": "concurrently \"npx tsc \" \"nodemon -q ./dist/server/index.js\""
  },
  "keywords": [],
